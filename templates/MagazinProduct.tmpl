                        <li><a href="#">Shirts</a></li>
                        <li><a href="#">Shoes</a></li>
                        <li><a href="#">Shorts</a></li>
                        <li><a href="#">Sweaters &amp; Knits</a></li>
                        <li><a href="#">T-Shirts</a></li>
                        <li><a href="#">Tanks</a></li>
                    </ul>
                </li>
            </ul>
        </aside>
        <section class="selection_menu">
            <section ><!-- class="form_selection" -->
                <form action="#" class="form_selection">
                    <fieldset class="trending_now">
                        <p>Trending now</p>
                        <!-- <button>Bohemian</button> -->
                        <input value="Bohemian" type="button">
                        <span>&nbsp;|&nbsp;</span>
                        <!-- <button>Floral</button> -->
                        <input value="Floral" type="button">
                        <span>&nbsp;|&nbsp;</span>
                        <!-- <button>Lace</button> -->
                        <input value="Lace" type="button">
                        <br>
                        <!-- <button>Floral</button> -->
                        <input value="Floral" type="button">
                        <span>&nbsp;|&nbsp;</span>
                        <!-- <button>Lace</button> -->
                        <input value="Lace" type="button">
                        <span>&nbsp;|&nbsp;</span>
                        <!-- <button>Bohemian</button> -->
                        <input value="Bohemian" type="button">
                    </fieldset>
                    <fieldset class="size">
                        <p>Size</p>
                        <div class="checkbox_box">
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkXXS" name="check">
                                <label for="checkXXS"><span>&nbsp;XXS</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkXS" name="check">
                                <label for="checkXS"><span>&nbsp;XS</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkS" name="check">
                                <label for="checkS"><span>&nbsp;S</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkM" name="check">
                                <label for="checkM"><span>&nbsp;M</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkL" name="check">
                                <label for="checkL"><span>&nbsp;L</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkXL" name="check">
                                <label for="checkXL"><span>&nbsp;XL</span></label>
                            </div>
                            <div class="checkbox_box_mini">
                                <input type="checkbox" id="checkXXL" name="check">
                                <label for="checkXXL"><span>&nbsp;XXL</span></label>
                            </div>
                        </div>
                    </fieldset>
                    <fieldset class="price">
                        <p>PRICE</p>
                        <div id="slider-range"></div>
                        <div class="cost">
                            <div class="cost_min">
                                <span>$<!-- 52 -->
                                    <input id="minCost" type="text" value="52" readonly>
                                </span>
                            </div>
                            <div class="cost_max">
                                <span>$<!-- 400 -->
                                    <input id="maxCost" type="text" value="400" readonly>
                                </span>
                            </div>
                        </div>
                    </fieldset>
                </form>
                
                <form class="buttons_block" action="/MagazinProduct">
                    <div class="block_one">
                        <button style="border-right: 2px solid #ef5b70;">Sort By</button>
                        <select ize="5" name="sort" style="height: 35px; padding: 0 10px;">
                            <option selected="selected" value="name">Name</option>
                            <option value="prise">Prise</option>
                        </select>
                    </div>
                    <div class="block_two">
                        <button style="border-right: 2px solid #ef5b70;">Show</button>
                        <select ize="5" name="show" style="height: 35px; padding: 0 10px;">
                            <option selected="selected" value="6">6</option>
                            <option value="12">12</option>
                            <option value="18">18</option>
                        </select>
                    </div>
                </form>
            </section>
        {% for good in goods %}
            <div class="content_box_two"><a href="MagazinSingle_Page" target="_blank"></a>
                <div class="img_in_box_two img_in_box_two_hover">
                    <img src="{{good.good_foto}}" alt="{{good.good_name}}">
                </div>
                <div class="hover_button_block2">
                    <button data-id="{{good.id}}" class="add_to_cart"><img src="/img/Add_to_Cart.png" alt="Add to Cart"></button>
                    <div class=hover_button_block_row_two>
                        <button><img src="/img/refresh.png" alt="refresh"></button>
                        <button><img src="/img/like.png" alt="like"></button>
                    </div>
                </div>
                <p class="product_name">{{good.good_name}}</p>
                <div class="stars"></div>
                <p class="product_price">$<span>{{good.good_price}}</span></p>
            </div>
        {% endfor %}
        {% if viewAll == true %}
            <nav class="selection_menu_nav_menu">
                <a href="/MagazinProduct">Back</a>
            </nav>
        {% else %}
            <nav class="selection_menu_nav_menu">
                <div class="pagination"><a href="#"><i class="fa fa-angle-left" aria-hidden="true"></i></a> <a href="#">1</a> <a href="#">2</a> <a href="#">3</a> <a href="#">4</a> <a href="#">5</a> <a href="#">6</a> <a href="#">.....</a> <a href="#">20</a> <a href="#"><i class="fa fa-angle-right" aria-hidden="true"></i></a></div>
                <!-- <button>View All</button> -->
                <a href="MagazinProduct/ViewAll">View All</a>
            </nav>
        {% endif  %}
        </section>
    </main>
    <section class="banner2_page1">
        <div class="banner2_content container">
            <div class="content_box_three content_box_three_product"><img src="/img/forma-3-1.png" alt="Free Delivery">
                <div class="text_in_box_three">
                    <h4>Free Delivery</h4>
                    <p>Worldwide delivery on all. Authorit tively morph next-generation innov tion with extensive models.</p>
                </div>
            </div>
            <div class="content_box_three content_box_three_product"><img src="/img/forma-3-2.png" alt="Sales &amp; discounts">
                <div class="text_in_box_three">
                    <h4>Sales &amp; discounts</h4>
                    <p>Worldwide delivery on all. Authorit tively morph next-generation innov tion with extensive models.</p>
                </div>
            </div>
            <div class="content_box_three content_box_three_product"><img src="/img/forma-3-3.png" alt="Quality assurance">
                <div class="text_in_box_three">
                    <h4>Quality assurance</h4>
                    <p>Worldwide delivery on all. Authorit tively morph next-generation innov tion with extensive models.</p>
                </div>
            </div>
        </div>
    </section>
    {%include 'pre-footer.tmpl'%}
    <footer>
        {%include 'footer.tmpl'%}
    </footer>
</body>

</html>